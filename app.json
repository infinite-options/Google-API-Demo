{
  "expo": {
    "name": "Google-API-Demo-ReactNative",
    "slug": "Google-API-Demo-ReactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "googleapidemoreactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.infiniteoptions.apidemo",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.infiniteoptions.apidemo",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.255360526528-j5g199i7073sojr0e5obteprc4icote4"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "webClientId": "255360526528-9f1t61ubeu4afpakudqkpp9gn8ljn32q.apps.googleusercontent.com",
      "androidClientId": "255360526528-j5g199i7073sojr0e5obteprc4icote4.apps.googleusercontent.com",
      "iosClientId": "255360526528-j5g199i7073sojr0e5obteprc4icote4.apps.googleusercontent.com",
      "clientSecret": "GOCSPX-EC5A"
    }
  }
}
